    "@types/vscode": "^1.47.0",
    "@types/node": "^14.0.26",
    "@types/qs": "^6.9.4",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.5.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "qs": "^6.9.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
