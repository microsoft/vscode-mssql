    "@types/node": "^14.0.26",
    "@types/qs": "^6.9.7",
    "@types/vscode": "^1.78.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "qs": "^6.9.7"
  }
}
