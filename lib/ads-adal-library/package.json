{
  "name": "@microsoft/ads-adal-library",
  "version": "1.0.17",
  "description": "ADAL NodeJS authentication library",
  "main": "dist/index.js",
  "repository": "git://github.com/microsoft/vscode-mssql.git",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn run build-ts && yarn run lint",
