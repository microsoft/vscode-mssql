{
    "name": "sql-database-projects-vscode",
    "displayName": "SQL Database Projects",
    "description": "Enables users to develop and publish database schemas for MSSQL Databases",
    "version": "1.5.8",
    "publisher": "ms-mssql",
    "preview": false,
    "homepage": "https://github.com/Microsoft/vscode-mssql/blob/main/extensions/sql-database-projects/README.md",
    "engines": {
        "vscode": "^1.98.0",
        "azdata": ">=1.47.0"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "icon": "images/sqlDatabaseProjects.png",
    "aiKey": "29a207bb14f84905966a8f22524cb730-25407f35-11b6-4d4e-8114-ab9e843cb52f-7380",
    "activationEvents": [
        "workspaceContains:**/*.sqlproj",
        "onView:dataworkspace.views.main"
    ],
    "main": "./out/src/extension.js",
    "l10n": "./l10n",
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/vscode-mssql.git"
    },
    "bugs": {
        "url": "https://github.com/Microsoft/vscode-mssql/issues"
    },
    "extensionDependencies": [
        "ms-mssql.mssql",
        "ms-mssql.data-workspace-vscode"
    ],
    "scripts": {
        "build:runtime-localization": "node ../../scripts/localization-generate.js sql-database-projects .",
        "build:extension": "tsc -p tsconfig.extension.json",
        "build:prepare": "yarn build:runtime-localization",
        "build": "node scripts/build.js",
        "watch:extension": "tsc -watch -p tsconfig.extension.json  --preserveWatchOutput",
        "watch": "yarn build:prepare && run-p -l watch:extension",
        "lint": "eslint ./src",
        "localization": "node ../../scripts/localization-extract.js sql-database-projects",
        "test": "yarn build && vscode-test --coverage",
        "package": "yarn build && vsce package --no-yarn"
    },
    "capabilities": {
        "virtualWorkspaces": false,
        "untrustedWorkspaces": {
            "supported": true
        }
    },
    "contributes": {
        "projects": [
            "sqlproj"
        ],
        "properties": {
          "filePath": {
            "type": "string",
            "description": "The path to the .sqlproj file."
          },
          "fileDisplayName": {
            "type": "string",
            "description": "The display name of the .sqlproj file."
          },
          "runCodeAnalysis": {
            "type": "boolean",
            "description": "Run code analysis on the SQLProj file. Optional, default is true."
          }
        }
      }
    ]
  },
  "dependencies": {
    "@microsoft/ads-extension-telemetry": "^3.0.1",
    "@xmldom/xmldom": "0.8.4",
    "ajv": "^6.12.6",
    "axios": "^1.12.0",
    "extract-zip": "^2.0.1",
    "fast-glob": "^3.2.7",
    "fs-extra": "^5.0.0",
    "promisify-child-process": "^3.1.1",
    "semver": "^7.5.2",
    "vscode-jsonrpc": "^8.2.1",
    "vscode-languageclient": "5.2.1",
    "which": "^2.0.2",
    "xml-formatter": "2.1.0"
  },
  "devDependencies": {
    "@microsoft/azdata-test": "^3.0.4",
    "@types/fs-extra": "^5.0.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^22.13.14",
    "@types/semver": "^7.3.1",
    "@types/sinon": "^9.0.4",
    "@types/vscode": "1.98.0",
    "@types/which": "^2.0.1",
    "@types/xml-formatter": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/l10n-dev": "^0.0.35",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.1.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "mocha": "^9.2.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^17.1.0",
    "prettier": "^3.3.3",
    "should": "^13.2.1",
    "sinon": "^9.0.2",
    "typemoq": "^2.1.0",
    "typescript": "^5.8.3"
  },
  "__metadata": {}
}
