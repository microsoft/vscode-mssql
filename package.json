{
  "name": "vscode-mssql",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for SQL Server extensions for Visual Studio Code",
  "workspaces": [
    "extensions/*"
  ],
  "resolutions": {
    "@types/react": "18.3.27"
  },
  "scripts": {
    "build": "yarn workspaces run build",
    "watch": "node scripts/watch-extensions.js",
    "localization": "node scripts/localization-extract.js",
    "prepare": "husky"
  },
  "devDependencies": {
    "@vscode/l10n-dev": "^0.0.35",
    "prettier": "^3.3.3",
    "husky": "^9.0.11"
  }
}
